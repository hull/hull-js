<div class="hull-quiz hull-quiz--result">
  <div class="hull-quiz__header">
    <h1 class="hull-quiz__title">Bravo</h1>
  </div>

  <div class="hull-quiz__result">
    {{#result.data}}
      <p class="hull-quiz__result__score">You Have <strong>{{score}}</strong> points !</p>
      <p class="hull-quiz__result__timing">It took <strong>{{timing}}</strong> seconds to answer !</p>
    {{/result.data}}
  </div>

  <div class="hull-pager">
    <div class="hull-pager__content">
      <button class="hull-btn" data-hull-action="share" data-hull-provider="twitter" data-hull-text="I just scored {{result.data.score}} on Quiz: {{result.name}}. Can you beat me?">Share your score on Twitter</button>
      <small>or</small>
      <span data-hull-action="start">Try again</span>
    </div>
  </div>

</div>